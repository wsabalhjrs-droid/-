{
  "name": "docs-site",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "tsx ./prebuild-scripts/index.ts",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 vike build",
    "quick-build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 vike build",
    "predev": "tsx ./prebuild-scripts/index.ts",
    "dev": "vike dev",
    "quick-dev": "vike dev",
    "sidebar": "tsx ./prebuild-scripts/prebuild-sidebar.ts",
    "integrations": "tsx ./prebuild-scripts/prebuild-integrations.ts",
    "start": "tsx server/index.ts",
    "typecheck": "tsc",
    "fmt:check": "biome check . --diagnostic-level=error && prettier --check .",
    "fmt": "biome check . --write --unsafe --diagnostic-level=error && prettier --write .",
    "test": "set VITEST=true && vitest run",
    "validate-codeblocks": "tsx scripts/validate-codeblocks/index.ts",
    "validate-codeblocks:warnings": "tsx scripts/validate-codeblocks/index.ts --warnings",
    "validate-codeblocks:quiet": "tsx scripts/validate-codeblocks/index.ts --quiet",
    "test:watch": "set VITEST=true && vitest watch",
    "clear-cache": "rm -rf .build node_modules/.cache",
    "clear-cache-win": "rmdir /s /q \".build\" && rmdir /s /q \"node_modules\\.cache\""
  },
  "dependencies": {
